  'Menu':
    'prefix': 'menu'
    'body': """
      ul.menu
        li
          a(href='#') Item One
        li
          a(href='#') Item Two
        li
          a(href='#') Item Three
      """
  'Dropdown Menu':
    'prefix': 'dropdown-menu'
    'body': """
      ul.dropdown.menu(data-dropdown-menu='')
        li
          a Item 1
          ul.menu
            li
              a(href='#') Item 1A Loooong
            li
              a(href='#')  Item 1 sub
              ul.menu
                li
                  a(href='#') Item 1 subA
                li
                  a(href='#') Item 1 subB
                li
                  a(href='#')  Item 1 sub
                  ul.menu
                    li
                      a(href='#') Item 1 subA
                    li
                      a(href='#') Item 1 subB
                li
                  a(href='#')  Item 1 sub
                  ul.menu
                    li
                      a(href='#') Item 1 subA
            li
              a(href='#') Item 1B
        li
          a(href='#') Item 2
          ul.menu
            li
              a(href='#') Item 2A
            li
              a(href='#') Item 2B
        li
          a(href='#') Item 3
        li
          a(href='#') Item 4
    """
  'Accordion Menu':
    'prefix': 'accordion-menu'
    'body': """
      ul.vertical.menu(data-accordion-menu='')
        li
          a(href='#') Item 1
          ul.menu.vertical.nested.is-active
            li
              a(href='#') Item 1A
              ul.menu.vertical.nested
                li
                  a(href='#') Item 1Ai
                li
                  a(href='#') Item 1Aii
                li
                  a(href='#') Item 1Aiii
            li
              a(href='#') Item 1B
            li
              a(href='#') Item 1C
        li
          a(href='#') Item 2
          ul.menu.vertical.nested
            li
              a(href='#') Item 2A
            li
              a(href='#') Item 2B
        li
          a(href='#') Item 3
    """
  'TopBar Menu':
    'prefix': 'topbar-menu'
    'body': """
      .top-bar
        .top-bar-left
          ul.dropdown.menu(data-dropdown-menu='')
            li.menu-text Site Title
            li.has-submenu
              a(href='#') One
              ul.submenu.menu.vertical(data-submenu='')
                li
                  a(href='#') One
                li
                  a(href='#') Two
                li
                  a(href='#') Three
            li
              a(href='#') Two
            li
              a(href='#') Three
        .top-bar-right
          ul.menu
            li
              input(type='search', placeholder='Search')
            li
              button.button(type='button') Search
    """
  'Responsive Menu':
    'prefix': 'responsive-menu'
    'body': """
