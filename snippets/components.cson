                  a(href='#') Item 1B
                li
                  a(href='#') Item 1C
                li
                  a(href='#') Item 1D
                li
                  a(href='#') Item 1E
            li
              a(href='#') Item 1B
        li.has-submenu
          a(href='#') Item 2
          ul.vertical.submenu.menu(data-submenu='')
            li
              a(href='#') Item 2A
            li
              a(href='#') Item 2B
        li.has-submenu
          a(href='#') Item 3
          ul.vertical.submenu.menu(data-submenu='')
            li
