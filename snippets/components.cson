".text.jade":
  'Button':
    'prefix': 'button'
    'body': """
      a.button(href='$0') $1
    """
  'Button Group':
    'prefix': 'button-group'
    'body': """
      .button-group
        a.button $1
    """
  'Secondary Button Group':
    'prefix': 'secondary-button-group'
    'body': """
      .expanded.button-group
        a.button $1
    """
  'Expanded Button Group':
    'prefix': 'expanded-button-group'
    'body': """
      .expanded.button-group
        a.button $1
    """
  'Stacked Button Group':
    'prefix': 'stacked-button-group'
    'body': """
      .stacked-for-small.button-group
        a.button $1
    """
  'Split Buttons':
    'prefix': 'split-buttons'
    'body': """
      .button-group
        a.button $1
        a.dropdown.button.arrow-only
          span  class=&quot;show-for-sr&quot;&gt;$2
    """
  'Close Button':
    'prefix': 'close-button'
    'body': """
      button.close-button(class='arial-label=', close='', type='button')
        span(aria-hidden='true') &amp;times&apos;
    """
  'Slider':
    'prefix': 'slider'
    'body': """
      .slider(data-slider='', data-initial-start='50', data-end='200')
        span.slider-handle(data-slider-handle='', role='slider', tabindex='1')
        span.slider-fill(data-slider-fill='')
        input(type='hidden')
    """
  'Slider Vertical':
    'prefix': 'slider-vertical'
    'body': """
